{
  "name": "@ethang/toolbelt",
  "version": "2.0.0",
  "main": "index.js",
  "author": "Ethan Glover",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "lint": "eslint . --fix",
    "test": "vitest run --coverage.enabled=true",
    "test:ui": "vitest --ui --coverage.enabled=true --open=false"
  },
  "eslintConfig": {
    "extends": "ethang",
    "ignorePatterns": [
      "coverage/**"
    ],
    "rules": {
      "complexity": "off",
      "max-depth": "off",
      "max-lines": "off",
      "max-lines-per-function": "off",
      "prefer-named-capture-group": "off",
      "require-unicode-regexp": "off"
    }
  },
  "dependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "convert": "^5.0.0",
    "idb": "^8.0.0",
    "lodash": "^4.17.21",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ethang/project-builder": "^1.1.28",
    "@tanstack/query-core": "^5.28.9",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.2",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "eslint-config-ethang": "^14.1.6",
    "type-fest": "^4.14.0",
    "vitest": "^1.4.0",
    "vitest-fetch-mock": "^0.2.2"
  },
  "peerDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "convert": "^5.0.0",
    "idb": "^8.0.0",
    "lodash": "^4.17.21",
    "zod": "^3.22.4"
  }
}
